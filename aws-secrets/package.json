{
  "name": "aws-secrets",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "generate-schema": "ts-json-schema-generator --path index.ts --type Secrets --tsconfig tsconfig.json > schema.json",
    "build": "esbuild index.ts --bundle --minify --platform=node --target=node16 --format=esm --sourcemap --outfile=dist/index.mjs"
  },
  "dependencies": {
    "@actions/core": "1.2.7",
    "@actions/github": "5.0.0",
    "@aws-sdk/client-secrets-manager": "^3.47.0",
    "@aws-sdk/client-ssm": "^3.47.0",
    "ajv": "^8.9.0",
    "esbuild": "^0.14.11",
    "js-yaml": "^4.1.0",
    "jsonpath-plus": "^6.0.1",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "14.14.37",
    "ts-json-schema-generator": "^0.97.0"
  }
}
