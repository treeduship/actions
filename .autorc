  "plugins": [
    "git-tag",
    "conventional-commits",
    "released",
    [
      "exec",
      {
        "afterRelease": "export ACTION_VERSION=$(jq -n 'env|.ARG_0|fromjson|.newVersion' -r) && npm run bump-docs --workspaces --if-present"
      }
    ]
  ],
  "owner": "uShip",
