  "plugins": [
    "git-tag",
    "conventional-commits",
    "released",
    [
      "exec",
      {
        "afterRelease": "ACTION_VERSION=\"$(echo \"$ARG_0\" | jq '.nextVersion' -r)\" npm run bump-docs --workspaces --if-present"
      }
    ]
  ],
